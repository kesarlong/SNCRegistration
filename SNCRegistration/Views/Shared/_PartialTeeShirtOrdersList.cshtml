@model IEnumerable<SNCRegistration.ViewModels.TeeShirtOrdersModel>

<tr>
    <td colspan="7">
        <h4>Total: @Model.Count()</h4>
    </td>
</tr>

@foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UnitChapterNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VolunteerFirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VolunteerLastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LeadContactFirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LeadContactLastName)
)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VolunteerShirtOrder)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VolunteerShirtSize)
        </td>
    </tr>
    }