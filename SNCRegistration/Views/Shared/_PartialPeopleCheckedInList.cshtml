@model IEnumerable<SNCRegistration.ViewModels.PeopleCheckedInCountModel>
<tr>
    <td colspan="5">
        <h4>Total: @Model.Count()</h4>
    </td>
</tr>

@foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UnitChapterNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Registrant)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParticipantFirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParticipantFirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CheckedIn)
        </td>
    </tr>
    }
  
