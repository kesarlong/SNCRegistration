@model IEnumerable<SNCRegistration.ViewModels.Event>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EventYear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Enrollment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Reporting)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EventYear)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Enrollment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Reporting)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EventYear }) |
            @Html.ActionLink("Details", "Details", new { id=item.EventYear }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EventYear })
        </td>
    </tr>
}

    
</table>
